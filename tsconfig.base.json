{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@ktn/core-form/*": ["libs/core-form/src/*"],
      "@ktn/core/*": ["libs/core/src/*"],
      "@ktn/hooks-form/*": ["libs/hooks-form/src/*"],
      "@ktn/hooks/*": ["libs/hooks/src/*"],
      "@ktn/theme/*": ["libs/theme/src/*"],
      "@ktn/ui-atoms/*": ["libs/ui-atoms/src/*"],
      "@ktn/ui-cells/*": ["libs/ui-cells/src/*"],
      "@ktn/ui-controls-core/*": ["libs/ui-controls-core/src/*"],
      "@ktn/ui-controls-mui/*": ["libs/ui-controls-mui/src/*"],
      "@ktn/ui-features/*": ["libs/ui-features/src/*"],
      "@ktn/ui-features/ide-shell": ["libs/ui-features/src/ide-shell/index.ts"],
      "@ktn/ui-molecules/*": ["libs/ui-molecules/src/*"],
      "@ktn/ui-molecules/ide": ["libs/ui-molecules/src/ide/index.ts"],
      "@ktn/ui-cells": ["libs/ui-cells/src/index.ts"]
    },
    "strict": true,
    "noImplicitReturns": true,
    "noImplicitOverride": true,
    "noUnusedLocals": true,
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": true,
    "target": "es2022",
    "module": "esnext",
    "moduleResolution": "bundler",
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "isolatedModules": true,
    "importHelpers": true,
    "customConditions": ["@ktn-studio/source"]
  }
}
